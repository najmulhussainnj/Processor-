### Makefile for the tilelink project
### Generated by Bluespec Workstation on Sat Mar 17 15:15:37 IST 2018

default: full_clean compile link simulate

.PHONY: compile
compile:
	@echo Compiling...
	bsc -u -sim -simdir tilelink_info -bdir tilelink_info -info-dir tilelink_info -keep-fires -p .:%/Prelude:%/Libraries:%/Libraries/BlueNoC -g mkTilelink  Tilelink.bsv 
	@echo Compilation finished

.PHONY: link
link:
	@echo Linking...
	bsc -e mkTilelink -sim -o ./out -simdir tilelink_info -p .:%/Prelude:%/Libraries:%/Libraries/BlueNoC -bdir tilelink_info -keep-fires 
	@echo Linking finished

.PHONY: simulate
simulate:
	@echo Simulation...
	./out 
	@echo Simulation finished

.PHONY: clean
clean:

.PHONY: full_clean
full_clean:
